@model IEnumerable<Meditrack.Models.CasaComercial>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreCasaComercial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DireccionCasaComercial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonoCasaComercial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CorreoCasaComercial)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WebCasaComercial)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreCasaComercial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DireccionCasaComercial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoCasaComercial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoCasaComercial)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WebCasaComercial)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCasaComercial">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCasaComercial">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCasaComercial">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
