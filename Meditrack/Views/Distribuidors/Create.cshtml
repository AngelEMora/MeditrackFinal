@model Meditrack.Models.Distribuidor

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<h4>Distribuidor</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NombreDistribuidor" class="control-label"></label>
                <input asp-for="NombreDistribuidor" class="form-control" />
                <span asp-validation-for="NombreDistribuidor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DireccionDistribuidor" class="control-label"></label>
                <input asp-for="DireccionDistribuidor" class="form-control" />
                <span asp-validation-for="DireccionDistribuidor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TelefonoDistribuidor" class="control-label"></label>
                <input asp-for="TelefonoDistribuidor" class="form-control" />
                <span asp-validation-for="TelefonoDistribuidor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Correo" class="control-label"></label>
                <input asp-for="Correo" class="form-control" />
                <span asp-validation-for="Correo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoDistribuidor" class="control-label"></label>
                <input asp-for="TipoDistribuidor" class="form-control" />
                <span asp-validation-for="TipoDistribuidor" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdCasaComercial" class="control-label"></label>
                <select asp-for="IdCasaComercial" class ="form-control" asp-items="ViewBag.IdCasaComercial"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
