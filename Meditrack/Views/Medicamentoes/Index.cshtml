@model IEnumerable<Meditrack.Models.Medicamento>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreMedicamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CantidadDisponibleMedicamentos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaVencimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Distribuidor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PuntosReorden)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lote)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaFabricacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdMovimientoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTipoMedicamentoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMedicamento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantidadDisponibleMedicamentos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaVencimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Distribuidor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PuntosReorden)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lote)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFabricacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdMovimientoNavigation.IdMovimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTipoMedicamentoNavigation.IdTipoMedicamento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMedicamento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMedicamento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMedicamento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
