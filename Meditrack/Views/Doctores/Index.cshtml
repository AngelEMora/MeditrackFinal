@model IEnumerable<Meditrack.Models.Doctore>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TelefonoDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EdadDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NacionalidadDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdentificacionDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SexoDoctor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Especialidad)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TelefonoDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EdadDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NacionalidadDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdentificacionDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SexoDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Especialidad)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdDoctor">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdDoctor">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdDoctor">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
